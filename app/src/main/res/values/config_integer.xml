    <integer name="paging_size">100</integer>
    <integer name="paging_startindex">1</integer>

    <!--默认的录歌时长-->
    <integer name="singtime">15</integer>
    <integer name="ksingwithmv_lowerlimit">30</integer>
    <integer name="cantata_lowerlimit">30</integer>
    <integer name="cantata_upperlimit">240</integer>

    <!--制作专辑：单曲数量限制-->
    <integer name="single_lowerlimit">5</integer>
    <integer name="single_upperlimit">10</integer>

    <!--清唱倒计时-->
    <integer name="cantata_countdown">5</integer>

    <!--出生年月限制-->
    <integer name="birth_limits">150</integer>

    <!--状态栏透明度-->
    <integer name="statusalpha_a">204</integer>

